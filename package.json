		"remark-rehype": "^10.1.0",
		"sass": "^1.54.3",
		"standard-version": "^9.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"to-vfile": "^7.2.3",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"unified": "^10.1.2",
		"vite": "^3.0.0"
	},
	"type": "module",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
