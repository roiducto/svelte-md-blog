		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "next",
		"@types/js-yaml": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"commitizen": "^4.2.5",
		"cz-conventional-changelog": "^3.3.0",
		"dayjs": "^1.11.4",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"js-yaml": "^4.1.0",
		"prettier": "^2.6.2",
